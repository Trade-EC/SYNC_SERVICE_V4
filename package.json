{
  "name": "hello_world",
  "version": "1.0.0",
  "description": "hello world sample for NodeJS",
  "main": "app.js",
  "repository": "https://github.com/awslabs/aws-sam-cli/tree/develop/samcli/local/init/templates/cookiecutter-aws-sam-hello-nodejs",
  "author": "SAM CLI",
  "license": "MIT",
  "scripts": {
    "unit": "jest",
    "lint": "eslint './**/*.ts' --max-warnings 0",
    "check-format": "prettier -c './**/*.ts'",
    "format": "prettier -w './**/*.ts'",
    "check-types": "tsc",
    "test": "yarn check-types && yarn unit",
    "build": "sam build",
    "validate": "sam validate",
    "start:local": "yarn build && sam local start-api",
    "deploy": "yarn validate && yarn build && sam deploy --guided",
    "list-endpoints": "sam list endpoints --output json",
    "clear:build": "rm -rf .aws-sam",
    "clear:modules": "rm -rf node_modules",
    "prepare": "npx husky install"
  },
  "dependencies": {
    "esbuild": "^0.14.14",
    "mongodb": "^5.8.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@commitlint/types": "^17.4.4",
    "@types/aws-lambda": "^8.10.92",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "commitizen": "^4.3.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "prettier": "^2.5.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
